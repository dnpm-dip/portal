{
    "name": "@dnpm-dip/kit",
    "type": "module",
    "version": "1.28.1",
    "description": "Kit for DNPM:DIP apps and packages.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs"
        }
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "rimraf ./dist && cross-env NODE_ENV=production npm run build:js && npm run build:types"
    },
    "keywords": [],
    "author": {
        "name": "Peter Placzek",
        "email": "peter.placzek1996@gmail.com",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KohlbacherLab/dnpm-dip.git",
        "directory": "packages/kit"
    },
    "bugs": {
        "url": "https://github.com/KohlbacherLab/dnpm-dip/issues"
    },
    "homepage": "https://github.com/KohlbacherLab/dnpm-dip#readme",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@nuxt/schema": "^4.1.3",
        "@nuxt/kit": "^4.2.2",
        "ufo": "^1.6.2"
    }
}
